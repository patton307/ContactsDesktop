<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox prefHeight="600.0" prefWidth="800.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
         <children>
            <HBox prefHeight="15.0" prefWidth="800.0">
               <children>
                  <TextField fx:id="textName" prefHeight="24.0" prefWidth="215.0" promptText="Name" />
                  <TextField fx:id="textPhone" prefHeight="24.0" prefWidth="215.0" promptText="Phone #" />
                  <TextField fx:id="textEmail" prefHeight="24.0" prefWidth="215.0" promptText="Email" />
                  <Button mnemonicParsing="false" onAction="#createContact" prefHeight="26.0" prefWidth="75.0" text="Add" />
                  <Button mnemonicParsing="false" onAction="#removeContact" prefHeight="26.0" prefWidth="80.0" text="Remove" />
               </children>
            </HBox>
            <ListView fx:id="listView" prefHeight="580.0" prefWidth="835.0" />
         </children>
      </VBox>
   </children>
</GridPane>
